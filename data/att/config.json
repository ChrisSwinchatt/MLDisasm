{
    "max_records":100,
    "gs_records":1000,
    "model":{
        "metrics":[
            "accuracy"
        ],
        "x_seq_len":15,
        "y_seq_len":64,
        "mask_value":null,
        "batch_size":50,
        "epochs":100,
        "kfolds":10,
        "shuffle":true,
        "input_size":256,
        "hidden_size":256,
        "output_size":128,
        "recurrent_unit":"lstm",
        "encoder_layers":1,
        "decoder_layers":1,
        "recurrent_activation":"tanh",
        "recurrent_use_bias":true,
        "recurrent_forget_bias":true,
        "dropout":0,
        "recurrent_dropout":0,
        "dense_activation":"sigmoid",
        "loss":"categorical_crossentropy",
        "optimizer":"SGD",
        "opt_params":{
            "lr":0.001
        }
    }
}